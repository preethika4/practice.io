//logic based problem output 20 60 104 152 204 260
#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    int c=4,d;
    int s1=20;
    int s2=40;
    
    if(n==1){
    printf("%d ",s1);
    }
    else{
        printf("%d ",s1);
        s1+=s2;
        printf("%d ",s1);
        
        for(int i=3;i<=n;i++){
            d=c*(i-2);
            s1+=s2+d;
        
        printf("%d ",s1);
        }
    }

    

    return 0;
}
