#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
     int last=0,sum=0;
     int last1=0,sum1=0;
        
       while(n>0){
        last=n%10;
        sum+=last;
        n/=10;
        
    }
    printf("%d ",sum);
    
    while(sum>0){
        last1=sum%10;
        sum1+=last1;
        sum/=10;
        
    }
    printf("%d ",sum1);
    



    return 0;
}
