// Counting the even and odd numbers
#include<stdio.h>
int main(){
    int n;
    int eventcount=0;
    int oddcount=0;
    scanf("%d \n",&n);
    int num;
    for(int i=0;i<n;i++){
        scanf("%d\n",&num);
    

    if(num%2==0){
        eventcount++;
    }
    else{
        oddcount++;
    }
}
    printf("%d %d",eventcount,oddcount);
    
}
